{
  "openapi": "3.0.1",
  "info": {
    "title": "WX.B2C.User.Verification.Api.Webhook",
    "description": "WX.B2C.User.Verification.Api.Webhook",
    "version": "1.0"
  },
  "paths": {
    "/healthcheck/ping": {
      "post": {
        "tags": [
          "HealthCheck"
        ],
        "operationId": "HealthCheck_Post",
        "parameters": [
          {
            "name": "api-version",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/onfido/events": {
      "post": {
        "tags": [
          "Onfido"
        ],
        "operationId": "Onfido_Post",
        "parameters": [
          {
            "name": "api-version",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OnfidoEventRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/OnfidoEventRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/OnfidoEventRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/passfort/events": {
      "post": {
        "tags": [
          "PassFort"
        ],
        "operationId": "PassFort_Post",
        "parameters": [
          {
            "name": "api-version",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PassFortEventRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/PassFortEventRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/PassFortEventRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ActionDto": {
        "required": [
          "action_type",
          "applications",
          "due",
          "task"
        ],
        "type": "object",
        "properties": {
          "action_type": {
            "type": "string"
          },
          "applications": {
            "type": "array",
            "items": {
              "x-nullable": false,
              "$ref": "#/components/schemas/ApplicationDto"
            }
          },
          "due": {
            "type": "string"
          },
          "task": {
            "$ref": "#/components/schemas/TaskDto"
          }
        }
      },
      "ApplicationDto": {
        "required": [
          "id",
          "product",
          "status"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "product": {
            "$ref": "#/components/schemas/ProductDto"
          },
          "status": {
            "type": "string"
          }
        }
      },
      "CheckCompletedData": {
        "required": [
          "check"
        ],
        "type": "object",
        "allOf": [
          {
            "$ref": "#/components/schemas/Data"
          }
        ],
        "properties": {
          "check": {
            "$ref": "#/components/schemas/CheckDto"
          }
        }
      },
      "CheckDto": {
        "required": [
          "check_type",
          "id",
          "result"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "check_type": {
            "type": "string"
          },
          "result": {
            "type": "string"
          }
        }
      },
      "Data": {
        "required": [
          "profile_id"
        ],
        "type": "object",
        "properties": {
          "customer_ref": {
            "type": "string",
            "x-nullable": true
          },
          "profile_id": {
            "type": "string"
          }
        }
      },
      "ManualActionRequiredData": {
        "required": [
          "actions"
        ],
        "type": "object",
        "allOf": [
          {
            "$ref": "#/components/schemas/Data"
          }
        ],
        "properties": {
          "actions": {
            "type": "array",
            "items": {
              "x-nullable": false,
              "$ref": "#/components/schemas/ActionDto"
            }
          }
        }
      },
      "Object": {
        "required": [
          "completed_at",
          "href",
          "id",
          "status"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "status": {
            "type": "string"
          },
          "completed_at": {
            "type": "string"
          },
          "href": {
            "type": "string"
          }
        }
      },
      "OnfidoEventRequest": {
        "required": [
          "payload"
        ],
        "type": "object",
        "properties": {
          "payload": {
            "$ref": "#/components/schemas/Payload"
          }
        }
      },
      "PassFortCheckCompletedEventRequest": {
        "required": [
          "data"
        ],
        "type": "object",
        "allOf": [
          {
            "$ref": "#/components/schemas/PassFortEventRequest"
          }
        ],
        "properties": {
          "data": {
            "$ref": "#/components/schemas/CheckCompletedData"
          }
        }
      },
      "PassFortEventRequest": {
        "required": [
          "event",
          "id",
          "timestamp"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "event": {
            "type": "string"
          },
          "timestamp": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "PassFortManualActionRequiredEventRequest": {
        "required": [
          "data"
        ],
        "type": "object",
        "allOf": [
          {
            "$ref": "#/components/schemas/PassFortEventRequest"
          }
        ],
        "properties": {
          "data": {
            "$ref": "#/components/schemas/ManualActionRequiredData"
          }
        }
      },
      "PassFortProductStatusChangedEventRequest": {
        "required": [
          "data"
        ],
        "type": "object",
        "allOf": [
          {
            "$ref": "#/components/schemas/PassFortEventRequest"
          }
        ],
        "properties": {
          "data": {
            "$ref": "#/components/schemas/ProductStatusChangedData"
          }
        }
      },
      "Payload": {
        "required": [
          "action",
          "object",
          "resource_type"
        ],
        "type": "object",
        "properties": {
          "resource_type": {
            "type": "string"
          },
          "action": {
            "type": "string"
          },
          "object": {
            "$ref": "#/components/schemas/Object"
          }
        }
      },
      "ProductDto": {
        "required": [
          "alias",
          "id",
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "alias": {
            "type": "string"
          }
        }
      },
      "ProductStatusChangedData": {
        "required": [
          "application_id",
          "new_status",
          "product"
        ],
        "type": "object",
        "allOf": [
          {
            "$ref": "#/components/schemas/Data"
          }
        ],
        "properties": {
          "application_id": {
            "type": "string"
          },
          "new_status": {
            "type": "string"
          },
          "product": {
            "$ref": "#/components/schemas/ProductDto"
          }
        }
      },
      "TaskDto": {
        "required": [
          "id",
          "state",
          "type",
          "variant"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "state": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "variant": {
            "$ref": "#/components/schemas/VariantDto"
          }
        }
      },
      "UnsupportedPassFortEventRequest": {
        "type": "object",
        "allOf": [
          {
            "$ref": "#/components/schemas/PassFortEventRequest"
          }
        ]
      },
      "VariantDto": {
        "required": [
          "alias",
          "id"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string",
            "x-nullable": true
          },
          "alias": {
            "type": "string"
          }
        }
      }
    }
  }
}
